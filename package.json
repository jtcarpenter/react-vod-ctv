{
  "name": "react-vod-ctv",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --history-api-fallback --content-base public/",
    "start:dist": "sudo webpack-dev-server --history-api-fallback --content-base dist/ --output-path dist/ --output-public-path dist/ --host 0.0.0.0 --port 80",
    "deploy": "NODE_ENV=prod webpack -p",
    "deploy:firetv": "NODE_ENV=prod PLATFORM=firetv webpack -p",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "author": "jtcarpenter",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.6"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.1.0",
    "css-loader": "^0.28.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.5.2",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "16",
    "sass-loader": "^6.0.3",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.3.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^PLATFORM(.*)$": "platforms/desktop$1"
